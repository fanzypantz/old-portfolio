{
  "connection": "default",
  "collectionName": "designs",
  "info": {
    "name": "designs"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "project_name": {
      "type": "string",
      "required": true
    },
    "images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "category": {
      "model": "category",
      "via": "designs"
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "index_image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "order": {
      "type": "integer",
      "required": true
    }
  }
}
